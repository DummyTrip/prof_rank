<html t:type="layout" title="SubjectDomain Index"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd"
      xmlns:p="tapestry:parameter">

<div>
    <h2>SubjectDomains</h2>

    <t:grid source="subjectDomains" row="subjectDomain" model="subjectDomainBeanModel">
        <p:showCell>
            <t:pagelink page="subjectdomain/showsubjectdomain" context="subjectDomain.id">Show</t:pagelink>
        </p:showCell>
        <p:editCell>
            <t:pagelink page="subjectDomain/editsubjectDomain" context="subjectDomain.id">Edit</t:pagelink>
        </p:editCell>
        <p:deleteCell>
            <t:actionlink t:id="delete" context="subjectDomain.id">Delete</t:actionlink>
        </p:deleteCell>
    </t:grid>

    <p><t:pagelink page="subjectDomain/createsubjectDomain">Create new subjectDomain</t:pagelink></p>

    <hr/>

</div>

</html>
