<html t:type="layout" title="Rulebook Index"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd"
      xmlns:p="tapestry:parameter">

<div>
    <h2>Rulebooks</h2>

    <t:grid source="rulebooks" row="rulebook" model="rulebookBeanModel">
        <p:editCell>
            <t:pagelink page="rulebook/editrulebook" context="rulebook.id">Edit</t:pagelink>
        </p:editCell>
        <p:deleteCell>
            <t:actionlink t:id="delete" context="rulebook.id">Delete</t:actionlink>
        </p:deleteCell>
    </t:grid>

    <p><t:pagelink page="rulebook/createrulebook">Create new rulebook</t:pagelink></p>

    <hr/>

</div>

</html>
