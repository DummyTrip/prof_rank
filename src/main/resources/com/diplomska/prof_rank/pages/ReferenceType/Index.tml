<html t:type="layout" title="ReferenceType Index"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd"
      xmlns:p="tapestry:parameter">

<div>
    <h2>ReferenceTypes</h2>

    <t:grid source="referenceTypes" row="referenceType" model="referenceTypeBeanModel">
        <p:showCell>
            <t:pagelink page="referencetype/show" context="referenceType.id">Show</t:pagelink>
        </p:showCell>
        <p:editCell>
            <t:pagelink page="referenceType/editreferenceType" context="referenceType.id">Edit</t:pagelink>
        </p:editCell>
        <p:deleteCell>
            <t:actionlink t:id="delete" context="referenceType.id">Delete</t:actionlink>
        </p:deleteCell>
    </t:grid>

    <p><t:pagelink page="referenceType/createreferenceType">Create new referenceType</t:pagelink></p>

    <hr/>


    <t:grid source="attributes" row="attribute" model="attributeBeanModel">
        <p:addCell>
            <t:actionlink t:id="add" context="attribute.id">Add</t:actionlink>
        </p:addCell>
    </t:grid>

</div>

</html>
