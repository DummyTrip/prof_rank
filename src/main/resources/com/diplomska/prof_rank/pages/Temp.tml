<div t:type='jquery/pagescroll' row='value' scroller='scroller' zone='zone' pageNumber="pageNumber">
    <t:delegate to="block:referenceElement"/>
</div>
<div class='zone' t:type='zone' t:id="zone"></div>
<div id='scroller'></div>



<t:block id="referenceElement">
    <a data-toggle="collapse" href="#${value.getId()}" aria-expanded="false" aria-controls="collapseExample" class="reference-link">
        <div class="row div-shadow white-background ref-list-padding">
            <div class="col-md-11">
                <h4 class="no-margin truncate-ref-link">${displayName}</h4>
            </div>
            <div class="col-md-1">
                <i class="ref-link-icon fa fa-chevron-down chevron-icon-size" aria-hidden="true"></i>
            </div>
        </div>
    </a>
    <div class="row collapse div-shadow" id="${value.getId()}">
        <div class="card card-block white-background no-padding-top">
            <table class="table-full-width">
                <tbody>
                <t:loop source="attributeValues" value="ari">
                    <tr class="table-row">
                        <th class="col-md-3 table-header th-td-padding">${ari.getAttribute().getName()}:</th>
                        <td class="col-md-9 th-td-padding">${ari.getValue()}</td>
                    </tr>
                </t:loop>
                </tbody>
            </table>

        </div>
    </div>

</t:block>


</html>
