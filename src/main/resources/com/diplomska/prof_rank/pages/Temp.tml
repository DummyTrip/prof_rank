<html t:type="layout" title="temp test page"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd"
      xmlns:p="tapestry:parameter" xmlns:T="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">

<div>
    <h2>Pages</h2>

    <ul>
        <!--<li><t:pagelink page="address/create">Create new address</t:pagelink></li>-->

    </ul>
</div>

<div>
    <t:grid source="temp" row="tmp"/>
</div>

<div>
    <t:loop source="temp" value="tmp">
        <t:beandisplay object="tmp"/>
    </t:loop>
</div>

<div>
    <t:form>
        <t:loop source="temp" value="tmp">
            <h3>${tmp}</h3>
            <t:loop source="attributes" value="attribute">
                <p>${attribute.getName()}</p>
                <t:if test="textInput">
                    <t:textfield value="testVal"/>
                </t:if>
            </t:loop>
        </t:loop>
    </t:form>

</div>

</html>
