<html t:type="layout" title="ReferenceType Index"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd"
      xmlns:p="tapestry:parameter">

<div class="row">

    <div class="col-md-offset-2 col-md-10">
        <h2 class="">Создај извештај</h2>
    </div>

</div>

<hr/>

<div class="row">
    <div class="col-md-12">

        <t:form t:id="form" class="form-horizontal">

            <div class="form-group">
                <label class="control-label col-sm-3">Од</label>
                <div class="col-sm-8">
                    <t:datefield value="startDate" validate="required"></t:datefield>
                </div>
            </div>

            <div class="form-group">
                <label class="control-label col-sm-3">До</label>
                <div class="col-sm-8">
                    <t:datefield value="endDate" validate="required"></t:datefield>
                </div>
            </div>

            <div class="form-group">
                <div class="col-sm-offset-3 col-sm-9">
                    <t:submit t:id="save" class="btn btn-primary btn-outline" value="Нов извештај"/>
                </div>
            </div>

        </t:form>

    </div>
</div>

<hr/>



</html>